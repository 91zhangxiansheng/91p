
# Pinout for the iCE40-HX8K Breakout Board

set_io clk J3

set_io flash_csb R12
set_io flash_clk R11
set_io flash_io0 P12
set_io flash_io1 P11
set_io flash_io2 T9  # center on J3
set_io flash_io3 P8  # center on J3

set_io ser_tx B12
set_io ser_rx B10

set_io leds[7] B5  # D9
set_io leds[6] B4  # D8
set_io leds[5] A2  # D7
set_io leds[4] A1  # D6
set_io leds[3] C5  # D5
set_io leds[2] C4  # D4
set_io leds[1] B3  # D3
set_io leds[0] C3  # D2

